{
    "app": {
        "name": "Club Samsara",
        "debug": false,
        "dbURL": "postgres://samsara:samsara@localhost:5432/samsara",
        "dbSSL": false,
        "sessionType": "local",
        "sessionSecret": "samsara",
        "redis":{
            "url": false,
            "tls": false
        },
        "secureOnly": false,
        "fontawesome": null,
        "cacheType": "redis"
    },
    "auth": {
        "google": {
            "clientID": "clientID",
            "clientSecret": "clientSecret",
            "callbackURL": "http://localhost:3000/auth/google/callback"
        },
        "intercode":{
            "authorizationURL": "authorizationURL",
            "tokenURL": "tokenURL",
            "clientID": null,
            "clientSecret": null,
            "callbackURL": "http://localhost:3000/auth/intercode/callback",
            "scope": "public openid",
            "passReqToCallback": true
        }
    },
    "aws": {
        "accessKeyId": "accessKeyId",
        "secretKey": "secretKey",
        "imageBucket": "imageBucket"
    },
    "game": {
        "defaultToPlayer": false,
        "brokerType": "redis",
        "channel": "samsara-game"
    },
    "webSocket": {
        "pingInterval": 30000,
        "debug": false
    },
    "server": {
        "cleanIntervalMins": 5
    },
    "jitsi": {
        "server": false,
        "active": false,
        "instance": {
            "region": "us-east-1",
            "id": false
        },
        "jwt": {
            "issuer": "samsara",
            "audience": "samsara-jitsi",
            "secret": null,
            "duration": 3600
        }
    }
}
